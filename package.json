{
  "name": "londontransit",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:bot": "npm --workspace apps/bot run dev",
    "dev:website": "npm --workspace apps/website run dev",
    "dev:docs": "npm --workspace apps/docs run dev",
    "dev": "concurrently \"npm run dev:bot\" \"npm run dev:website\""
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}